{
  "name": "touchpanel-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "archive": "ch5-cli archive -p ch5-react-ts-template -d dist -o archive",
    "send:panel": "ch5-cli deploy -p -H 10.1.10.16 -t touchscreen archive/ch5-react-ts-template.ch5z --slow-mode",
    "deploy": "npm run build && npm run archive && npm run send:panel"
  },
  "dependencies": {
    "@crestron/ch5-crcomlib": "^2.9.0",
    "@crestron/ch5-webxpanel": "^2.8.0",
    "@pepperdash/ch5-crcomlib-lite": "^2.10.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite-plugin-pwa": "^0.20.0",
    "vite-plugin-static-copy": "^1.0.6"
  },
  "devDependencies": {
    "@crestron/ch5-shell-utilities-cli": "^2.9.0",
    "@crestron/ch5-utilities-cli": "^2.0.0",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "prettier": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.19.7",
    "vite-plugin-singlefile": "^0.13.5",
    "vite-plugin-static-copy": "^1.0.1"
  }
}
